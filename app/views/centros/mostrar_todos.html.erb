<h1>Lista de los Centros de Salud</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Longitud</th>
    <th>Latitud</th>
    <th>Tipo</th>
    <th>Horario</th>
    <th>Descripcion</th>
  </tr>
 
  <% @centros.each do |centro| %>
    <tr>
      <td><%= centro.nombre %></td>
      <td><%= centro.longitud %></td>
      <td><%= centro.latitud %></td>
      <td><%= centro.tipo %></td>
      <td><%= centro.horario %></td>
      <td><%= centro.descripcion %></td>
      </tr>
  <% end %>
</table>


<%= link_to 'DocTico', "/" %>